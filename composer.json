{
    "name": "endeavors/maxmd-proofing",
    "description": "Provide an easy api to integrate with maxmd",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rodriguez",
            "email": "adam_rodriguez@bitbucket.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "endeavors/maxmd-http": "0.3.*",
        "endeavors/maxmd-apiauth": "0.1.*",
        "endeavors/maxmd-support": "~1.4.0",
        "endeavors/support-vo": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\MaxMD\\Proofing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\MaxMD\\Proofing\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
